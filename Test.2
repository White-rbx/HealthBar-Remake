-- ===== Utilities =====
local function trim(s) return (tostring(s or ""):gsub("^%s+", ""):gsub("%s+$", "")) end
local function redactKey(k)
    if not k then return "<nil>" end
    local s = tostring(k)
    if #s <= 12 then return "********" end
    return s:sub(1,6) .. "..." .. s:sub(-6)
end

local function split_args_parentheses(s)
    if not s then return {} end
    local inside = s:match("%((.*)%)")
    if not inside then return {} end
    local parts = {}
    local cur = ""
    local depth = 0
    local inq = false
    local qchar = nil
    for i = 1, #inside do
        local ch = inside:sub(i,i)
        if inq then
            cur = cur .. ch
            if ch == qchar then inq = false; qchar = nil end
        else
            if ch == '"' or ch == "'" then
                inq = true; qchar = ch; cur = cur .. ch
            elseif ch == "(" then
                depth = depth + 1; cur = cur .. ch
            elseif ch == ")" then
                depth = math.max(0, depth - 1); cur = cur .. ch
            elseif ch == "," and depth == 0 then
                table.insert(parts, trim(cur)); cur = ""
            else
                cur = cur .. ch
            end
        end
    end
    if trim(cur) ~= "" then table.insert(parts, trim(cur)) end
    for i = 1, #parts do
        parts[i] = parts[i]:gsub('^%s*"', ""):gsub('"%s*$', "")
        parts[i] = parts[i]:gsub("^%s*'", ""):gsub("'%s*$", "")
    end
    return parts
end

local function try_parse_parentheses_content(txt)
    if not txt then return nil end
    local inside = txt:match("%((.*)%)")
    if inside then
        inside = inside:gsub('^%s*"', ""):gsub('"%s*$', "")
        inside = inside:gsub("^%s*'", ""):gsub("'%s*$", "")
        return trim(inside)
    end
    return nil
end

local function findPlayerByName(name)
    if not name or name == "" then return nil end
    name = trim(name)
    local p = Players:FindFirstChild(name)
    if p then return p end
    for _,pl in ipairs(Players:GetPlayers()) do
        if pl.Name:lower():find(name:lower()) then return pl end
    end
    return nil
end

local function findObjectByName(name)
    if not name then return nil end
    local obj = workspace:FindFirstChild(name, true)
    if obj then return obj end
    for _,svc in ipairs(game:GetChildren()) do
        if svc and type(svc.FindFirstChild) == "function" then
            local f = svc:FindFirstChild(name, true)
            if f then return f end
        end
    end
    return nil
end

local function safeEval(expr)
    if not expr or expr == "" then return nil, "empty" end
    local f, err = loadstring("return " .. expr)
    if not f then return nil, err end
    local ok, res = pcall(f)
    if not ok then return nil, res end
    return res, nil
end

-- resolve locate string to instance (best-effort)
local function resolveLocate(str)
    if not str or str == "" then return nil, "empty" end
    local s = trim(str)
    -- try direct loadstring
    local ok, f = pcall(function() return loadstring("return " .. s) end)
    if ok and f then
        local ok2, val = pcall(f)
        if ok2 and val then return val, nil end
    end
    -- try workspace lookup
    local found = findObjectByName(s)
    if found then return found, nil end
    return nil, "resolve_failed"
end

-- ===== Executor HTTP detection & wrapper =====
local function detect_executor_request()
    if syn and syn.request then return syn.request end
    if http and http.request then return http.request end
    if http_request then return http_request end
    if request then return request end
    if fluxus and fluxus.request then return fluxus.request end
    return nil
end

local executor_request_fn = detect_executor_request()

local function executorRequest(opts)
    if not executor_request_fn then return nil, "no_executor_http" end
    local ok, res = pcall(function() return executor_request_fn(opts) end)
    if not ok then return nil, tostring(res) end
    -- normalize
    if type(res) == "string" then
        return { Body = tostring(res), StatusCode = 200, Success = true }, nil
    end
    local Body = res.Body or res.body or res.response or res.data or tostring(res)
    local StatusCode = res.StatusCode or res.status or res.statusCode or 0
    local Success = (res.Success ~= nil and res.Success) or (type(StatusCode) == "number" and StatusCode >= 200 and StatusCode < 300)
    return { Body = tostring(Body or ""), StatusCode = StatusCode, Success = Success }, nil
end

-- ===== OpenAI client (executor-based) =====
local function callOpenAI_local(prompt, key, project)
    if not prompt or trim(prompt) == "" then return nil, "empty_prompt" end
    key = key or STATE.OPENAI_KEY
    if not key or trim(key) == "" then return nil, "missing_api_key" end

    local payload = {
        model = STATE.MODEL,
        messages = { { role = "user", content = tostring(prompt) } },
        max_tokens = 700,
        temperature = 0.7,
    }
    local body = HttpService:JSONEncode(payload)
    local headers = { ["Content-Type"] = "application/json", ["Authorization"] = "Bearer " .. tostring(key) }
    if project and project ~= "" then headers["OpenAI-Project"] = tostring(project) end
    local res, err = executorRequest({
        Url = "https://api.openai.com/v1/chat/completions",
        Method = "POST",
        Headers = headers,
        Body = body,
    })
    if not res then return nil, ("http_failed_pcall: %s"):format(tostring(err)) end
    if not res.Success then
        local parsed = nil
        pcall(function() parsed = HttpService:JSONDecode(res.Body or "{}") end)
        local msg = parsed and parsed.error and parsed.error.message or ("status:"..tostring(res.StatusCode))
        return nil, ("http_error_%s: %s"):format(tostring(res.StatusCode), tostring(msg))
    end
    local decoded = nil
    local okd, derr = pcall(function() decoded = HttpService:JSONDecode(res.Body or "{}") end)
    if not okd or not decoded then return nil, "json_decode_failed" end
    local content = nil
    pcall(function()
        if decoded.choices and decoded.choices[1] and decoded.choices[1].message and decoded.choices[1].message.content then
            content = tostring(decoded.choices[1].message.content)
        end
    end)
    if not content then return nil, "no_content" end
    return content, nil
end

