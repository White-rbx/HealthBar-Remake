-- ===== Command handler =====
local function handleCommand(raw)
    if not raw or raw:sub(1,1) ~= "/" then return false end
    local cmd, rest = raw:match("^/(%S+)%s*(.*)$")
    cmd = (cmd or ""):upper()
    rest = rest or ""

    -- /API
    if cmd == "API" then
        local inside = try_parse_parentheses_content(rest) or trim(rest)
        if inside == "" then appendSystemMsg('Usage: /API ("sk-...")'); return true end
        STATE.OPENAI_KEY = inside
        appendSystemMsg(("API key set: %s (client-side)"):format(redactKey(inside)))
        return true
    end

    -- /APIJ
    if cmd == "APIJ" then
        local inside = try_parse_parentheses_content(rest) or trim(rest)
        if inside == "" then appendSystemMsg('Usage: /APIJ ("proj_...")'); return true end
        STATE.PROJECT_ID = inside
        appendSystemMsg(("Project set: %s"):format(tostring(inside)))
        return true
    end

    -- /HttpService
    if cmd == "HTTPSERVICE" then
        local ok, hs = pcall(function() return HttpService end)
        if not ok or not hs then appendSystemMsg("HttpService unavailable in this environment.", {255,80,80}); return true end
        local enabled = false; pcall(function() enabled = HttpService.HttpEnabled end)
        appendSystemMsg(("HttpService.HttpEnabled = %s | executorHTTP = %s"):format(tostring(enabled), tostring(executor_request_fn ~= nil)))
        return true
    end

    -- /HttpAsync
    if cmd == "HTTPASYNC" then
        local val = trim(rest):lower()
        if val == "true" then STATE.httpAsync = true; appendSystemMsg("HttpAsync = true"); return true end
        if val == "false" then STATE.httpAsync = false; appendSystemMsg("HttpAsync = false"); return true end
        appendSystemMsg('Usage: /HttpAsync true|false'); return true
    end

    -- /HttpState
    if cmd == "HTTPSTATE" then
        local target = try_parse_parentheses_content(rest) or trim(rest)
        if target == "" then appendSystemMsg('Usage: /HttpState ("example.com" or "https://example.com")'); return true end
        if not executor_request_fn then appendSystemMsg("No executor HTTP available to check site.", {255,80,80}); return true end
        local url = target:match("^https?://") and target or ("https://"..target)
        appendSystemMsg(("Checking %s ..."):format(url))
        task.spawn(function()
            local res, err = executorRequest({ Url = url, Method = "GET", Headers = { ["User-Agent"] = "Roblox-ChatGPT-Check" } })
            if not res then appendSystemMsg(("HTTP failed: %s"):format(tostring(err)), {255,80,80}); return end
            appendSystemMsg(("HTTP status: %s | success: %s"):format(tostring(res.StatusCode), tostring(res.Success)))
        end)
        return true
    end

    -- /Help
    if cmd == "HELP" or cmd == "H" then
        showHelpDetailed()
        return true
    end

    -- /Keep
    if cmd == "KEEP" then
        local content = try_parse_parentheses_content(rest) or trim(rest)
        if content == "" then appendSystemMsg('Usage: /Keep ("Text")'); return true end
        table.insert(savedKeeps, content)
        appendSystemMsg(("Saved note #%d"):format(#savedKeeps))
        return true
    end
    if cmd == "SHOWKEEP" then
        if #savedKeeps == 0 then appendSystemMsg("No saved notes.") else
            appendSystemMsg("Saved notes:")
            for i,v in ipairs(savedKeeps) do appendSystemMsg(("#%d: %s"):format(i, tostring(v))) end
        end
        return true
    end
    if cmd == "CLEARALLKEEP" or cmd == "CAK" then
        savedKeeps = {}
        appendSystemMsg("All saved notes cleared.")
        return true
    end

    -- /ClearTexts
    if cmd == "CLEARTEXTS" then
        for _,c in ipairs(gptChat:GetChildren()) do
            if c:IsA("Frame") then pcall(function() c:Destroy() end) end
        end
        appendSystemMsg("Cleared chat messages.")
        return true
    end

    -- /TextsCounts
    if cmd == "TEXTSCOUNTS" then
        appendSystemMsg(("Chat items: %d"):format(#gptChat:GetChildren()))
        return true
    end

    -- /CheckPlaHP
    if cmd == "CHECKPLAHP" then
        local inside = try_parse_parentheses_content(rest) or trim(rest)
        if inside == "" then appendSystemMsg('Usage: /CheckPlaHP ("Player")'); return true end
        local pl = findPlayerByName(inside)
        if not pl or not pl.Character then appendSystemMsg(("Player '%s' not found or missing character."):format(inside), {255,160,60}); return true end
        local hum = pl.Character:FindFirstChildOfClass("Humanoid")
        if not hum then appendSystemMsg(("Player '%s' has no Humanoid."):format(pl.Name), {255,160,60}); return true end
        appendSystemMsg(("%s HP: %.2f / %.2f"):format(pl.Name, hum.Health, hum.MaxHealth))
        return true
    end

    -- /CheckPlaAround
    if cmd == "CHECKPLAAROUND" or cmd == "CHECKPLAAROUND" then
        local radius = 50
        local meChar = LocalPlayer and LocalPlayer.Character
        local meRoot = meChar and (meChar:FindFirstChild("HumanoidRootPart") or meChar.PrimaryPart)
        if not meRoot then appendSystemMsg("Cannot find your HRP/character.", {255,160,60}); return true end
        local nearby = {}
        for _,pl in ipairs(Players:GetPlayers()) do
            if pl ~= LocalPlayer and pl.Character then
                local root = pl.Character:FindFirstChild("HumanoidRootPart") or pl.Character.PrimaryPart
                if root then
                    local dist = (root.Position - meRoot.Position).Magnitude
                    if dist <= radius then table.insert(nearby, {pl=pl, d=dist}) end
                end
            end
        end
        if #nearby == 0 then appendSystemMsg("No players within 50 studs.") else
            table.sort(nearby, function(a,b) return a.d < b.d end)
            for _,v in ipairs(nearby) do appendSystemMsg(("%s ‚Äî %.1f studs"):format(v.pl.Name, v.d)) end
        end
        return true
    end

    -- /Destroy
    if cmd == "DESTROY" then
        local args = split_args_parentheses(raw)
        if #args < 1 then appendSystemMsg('Usage: /Destroy ("locate")'); return true end
        local locate = args[1]
        local obj, err = resolveLocate(locate)
        if not obj then appendSystemMsg(("Locate failed: %s"):format(tostring(err)), {255,80,80}); return true end
        pcall(function() obj:Destroy() end)
        appendSystemMsg(("Destroyed %s"):format(tostring(locate)))
        return true
    end

    -- /Ins (create instance)
    if cmd == "INS" then
        local args = split_args_parentheses(raw)
        if #args < 3 then appendSystemMsg('Usage: /Ins ("Class","Name","ParentLocate","R,G,B","Vector3")'); return true end
        local className, setName, parentLoc, colStr, sizeStr = args[1], args[2], args[3], args[4], args[5]
        local parent = workspace
        if parentLoc and parentLoc ~= "" then
            local res, err = resolveLocate(parentLoc)
            if not res then appendSystemMsg(("Parent resolve failed: %s"):format(tostring(err)), {255,80,80}); return true end
            parent = res
        end
        local ok, inst = pcall(function() return Instance.new(className) end)
        if not ok or not inst then appendSystemMsg(("Failed to create class %s"):format(tostring(className)), {255,80,80}); return true end
        pcall(function() inst.Name = setName end)
        pcall(function() inst.Parent = parent end)
        if colStr and colStr:match("%d") then
            local nums = {}
            for n in colStr:gmatch("%d+") do table.insert(nums, tonumber(n)) end
            if #nums == 3 then pcall(function() if inst:IsA("BasePart") then inst.Color = Color3.fromRGB(nums[1], nums[2], nums[3]) end end) end
        end
        if sizeStr and sizeStr:match("[%d%.,%-]") then
            local v, e = safeEval(sizeStr)
            if v and typeof(v) == "Vector3" then pcall(function() if inst:IsA("BasePart") then inst.Size = v end end) end
        end
        appendSystemMsg(("Created %s named %s"):format(tostring(className), tostring(setName)))
        return true
    end

    -- /ReColor, /ReSize, /Repos
    if cmd == "RECOLOR" then
        local args = split_args_parentheses(raw)
        if #args < 2 then appendSystemMsg('Usage: /ReColor ("locate","R,G,B")'); return true end
        local locate, colorStr = args[1], args[2]
        local obj, err = resolveLocate(locate)
        if not obj then appendSystemMsg(("Locate failed: %s"):format(tostring(err)), {255,80,80}); return true end
        local nums = {}
        for n in colorStr:gmatch("%d+") do table.insert(nums, tonumber(n)) end
        if #nums == 3 then
            local col = Color3.fromRGB(nums[1], nums[2], nums[3])
            pcall(function()
                if obj:IsA("BasePart") then obj.Color = col end
                pcall(function() if obj.BackgroundColor3 then obj.BackgroundColor3 = col end end)
                pcall(function() if obj.TextColor3 then obj.TextColor3 = col end end)
            end)
            appendSystemMsg(("Color applied to %s"):format(tostring(locate)))
        else
            appendSystemMsg("Color parse failed.")
        end
        return true
    end

    if cmd == "RESIZE" or cmd == "RESIZE" then
        local args = split_args_parentheses(raw)
        if #args < 2 then appendSystemMsg('Usage: /ReSize ("locate","Vector3(x,y,z)")'); return true end
        local locate, vecStr = args[1], args[2]
        local obj, err = resolveLocate(locate)
        if not obj then appendSystemMsg(("Locate failed: %s"):format(tostring(err)), {255,80,80}); return true end
        local vec, e = safeEval(vecStr)
        if not vec or typeof(vec) ~= "Vector3" then appendSystemMsg(("Vector parse failed: %s"):format(tostring(e or "invalid"))); return true end
        pcall(function() if obj:IsA("BasePart") then obj.Size = vec end if obj.Size then obj.Size = vec end end)
        appendSystemMsg(("Resized %s to %s"):format(tostring(locate), tostring(vec)))
        return true
    end

    if cmd == "REPOS" or cmd == "REPOSITION" then
        local args = split_args_parentheses(raw)
        if #args < 2 then appendSystemMsg('Usage: /Repos ("locate","Vector3(x,y,z)")'); return true end
        local locate, vecStr = args[1], args[2]
        local obj, err = resolveLocate(locate)
        if not obj then appendSystemMsg(("Locate failed: %s"):format(tostring(err)), {255,80,80}); return true end
        local vec, e = safeEval(vecStr)
        if not vec or typeof(vec) ~= "Vector3" then appendSystemMsg(("Vector parse failed: %s"):format(tostring(e or "invalid"))); return true end
        pcall(function()
            if obj:IsA("BasePart") then obj.CFrame = CFrame.new(vec) end
            if obj.Position then obj.Position = vec end
        end)
        appendSystemMsg(("Moved %s to %s"):format(tostring(locate), tostring(vec)))
        return true
    end

    -- /View /Spy
    if cmd == "VIEW" or cmd == "SPY" then
        local inside = try_parse_parentheses_content(rest) or trim(rest)
        if inside == "" then appendSystemMsg('Usage: /View ("Player") or /Spy ("pla:Name" or "obj:workspace.Part")'); return true end
        if inside:lower():sub(1,4) == "pla:" then
            local name = trim(inside:sub(5))
            if name == "self" then startViewPlayer(LocalPlayer); return true end
            local pl = findPlayerByName(name)
            if not pl then appendSystemMsg("Player not found."); return true end
            startViewPlayer(pl)
            return true
        elseif inside:lower():sub(1,4) == "obj:" then
            local path = trim(inside:sub(5))
            local obj, err = resolveLocate(path)
            if not obj then appendSystemMsg(("Locate failed: %s"):format(err), {255,80,80}); return true end
            -- view object
            local cam = workspace.CurrentCamera
            viewState.prevType = cam.CameraType
            viewState.prevSubject = cam.CameraSubject
            viewState.active = true
            viewState.conn = RunService.RenderStepped:Connect(function()
                if not obj or not obj.Parent then
                    if viewState.conn then viewState.conn:Disconnect(); viewState.conn = nil end
                    viewState.active = false
                    return
                end
                cam.CameraType = Enum.CameraType.Scriptable
                local cf = (obj:IsA("BasePart") and obj.CFrame) or (obj.CFrame or CFrame.new(obj.Position or Vector3.new()))
                cam.CFrame = cf * CFrame.new(0,2,6)
            end)
            appendSystemMsg("Viewing object. Use /UnView to stop.")
            return true
        else
            local pl = findPlayerByName(inside)
            if pl then startViewPlayer(pl) else appendSystemMsg("Target not found."); end
            return true
        end
    end

    if cmd == "UNVIEW" or cmd == "UNSPY" then
        stopView()
        return true
    end

    -- /TP (client local)
    if cmd == "TP" or cmd == "TELEPORT" then
        local inside = try_parse_parentheses_content(rest) or trim(rest)
        if inside == "" then appendSystemMsg('Usage: /TP ("Vector3(x,y,z)")'); return true end
        local vec, err = safeEval(inside)
        if not vec or typeof(vec) ~= "Vector3" then appendSystemMsg(("Vector parse failed: %s"):format(tostring(err)), {255,80,80}); return true end
        local ch = LocalPlayer.Character
        if ch then
            local root = ch:FindFirstChild("HumanoidRootPart") or ch.PrimaryPart
            if root then pcall(function() root.CFrame = CFrame.new(vec) end); appendSystemMsg(("Teleported locally to %s"):format(tostring(vec))) else appendSystemMsg("No HRP to set CFrame.") end
        else appendSystemMsg("No character to teleport.") end
        return true
    end

    -- /Rejoin
    if cmd == "REJOIN" or cmd == "RJ" then
        local TeleportService = game:GetService("TeleportService")
        local placeId = game.PlaceId
        appendSystemMsg("Attempting to rejoin (may be blocked).")
        pcall(function() TeleportService:Teleport(placeId, LocalPlayer) end)
        return true
    end

    -- /LeaveGame
    if cmd == "LEAVEGAME" or cmd == "EXIT" then
        appendSystemMsg("Leaving game...")
        pcall(function() LocalPlayer:Kick("") end)
        return true
    end

    -- /Reset
    if cmd == "RESET" or cmd == "RE" then
        local ch = LocalPlayer.Character
        if ch then
            local hum = ch:FindFirstChildOfClass("Humanoid")
            if hum then pcall(function() hum.Health = 0 end); appendSystemMsg("Character reset.") else pcall(function() ch:BreakJoints() end); appendSystemMsg("Character broken.") end
        else appendSystemMsg("No character to reset.") end
        return true
    end

    -- /AutoBot (simple local emote/jump)
    if cmd == "AUTOBOT" then
        local inside = try_parse_parentheses_content(rest) or trim(rest)
        if inside:lower():find("true") then
            if autoBotState.enabled then appendSystemMsg("AutoBot already enabled.") else
                autoBotState.enabled = true
                appendSystemMsg("AutoBot enabled.")
                autoBotState.conn = RunService.Heartbeat:Connect(function()
                    if math.random(1,600) == 1 then
                        pcall(function()
                            local ch = LocalPlayer.Character
                            if ch then
                                local hum = ch:FindFirstChildOfClass("Humanoid")
                                if hum then hum:ChangeState(Enum.HumanoidStateType.Jumping) end
                            end
                        end)
                    end
                end)
            end
            return true
        elseif inside:lower():find("false") then
            if autoBotState.conn then pcall(function() autoBotState.conn:Disconnect() end); autoBotState.conn = nil end
            autoBotState.enabled = false
            appendSystemMsg("AutoBot disabled.")
            return true
        else
            appendSystemMsg('Usage: /AutoBot ("true" or "false")'); return true
        end
    end

    -- /console -> open developer console (Desktop)
    if cmd == "CONSOLE" then
        local arg = try_parse_parentheses_content(rest) or trim(rest)
        if arg ~= "" then
            local low = arg:lower()
            if low == "false" or low == "off" then
                pcall(function() StarterGui:SetCore("DevConsoleVisible", false) end)
                appendSystemMsg("Developer Console closed.")
            else
                pcall(function() StarterGui:SetCore("DevConsoleVisible", true) end)
                appendSystemMsg("Developer Console opened.")
            end
        else
            pcall(function() StarterGui:SetCore("DevConsoleVisible", true) end)
            appendSystemMsg("Developer Console opened.")
        end
        return true
    end

    -- Unknown command
    appendSystemMsg(("Unknown command: /%s ‚Äî use /Help"):format(tostring(cmd)))
    return true
end

-- ===== Sending chat flow (GPT) =====
local function sendChatMessage(text)
    text = tostring(text or "")
    if text:match("^%s*$") then return end
    if text:sub(1,1) == "/" then
        if handleCommand(text) then chatBox.Text = ""; return end
    end
    if not STATE.OPENAI_KEY or STATE.OPENAI_KEY == "" then
        appendSystemMsg("Error HttpService: Did you forget to put API?", {255,80,80})
        chatBox.Text = ""
        return
    end
    if STATE.requestInFlight then appendSystemMsg("Waiting for previous response..."); return end

    appendChat(true, text)
    chatBox.Text = ""
    local thinkingFr = appendChat(false, "[System] ‚åõ Thinking...", {255,200,60})
    STATE.requestInFlight = true
    task.spawn(function()
        local reply, err = callOpenAI_local(text, STATE.OPENAI_KEY, STATE.PROJECT_ID)
        pcall(function() if thinkingFr and thinkingFr.Parent then thinkingFr:Destroy() end end)
        STATE.requestInFlight = false
        if reply then appendChat(false, reply) else appendChat(false, ("[System] Error: %s"):format(tostring(err)), {255,80,80}) end
    end)
end

-- Bind UI
sendBtn.MouseButton1Click:Connect(function() sendChatMessage(chatBox.Text) end)
chatBox.FocusLost:Connect(function(enter) if enter then sendChatMessage(chatBox.Text) end end)
