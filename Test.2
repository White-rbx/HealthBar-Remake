                        if ui_hbm then ui_hbm.Visible = false end
                        if ui_OC  then ui_OC.Visible  = false end
                        if ui_gpt then ui_gpt.Visible = false end
                    end)

                    -- ‡πÅ‡∏™‡∏î‡∏á wa1 ‡πÅ‡∏•‡∏∞ wl
