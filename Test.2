        if hasHRP then
            timerMissing = 0
            if not isOpen then
                print("[HRP-Watcher] HRP detected -> opening UI")
                if ui_mtb and ui_mtb.Parent then
                    safeTween(ui_mtb, { Position = UDim2.new(0.48, 0, ui_mtb.Position.Y.Scale, ui_mtb.Position.Y.Offset) }, 0.28, Enum.EasingStyle.Quad)
