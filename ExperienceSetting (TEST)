-- ===== chat append helpers =====
local chatCount = 0
local function appendChat(isUser, text, colorTbl)
    chatCount = chatCount + 1
    local fr = Instance.new("Frame")
    fr.Name = "ChatItem_" .. chatCount
    fr.BackgroundTransparency = 1
    fr.Size = UDim2.new(1,0,0,0)
    fr.AutomaticSize = Enum.AutomaticSize.Y
    fr.Parent = gptChat

    local lbl = Instance.new("TextLabel")
    lbl.Name = "ChatLabel"
    lbl.Size = UDim2.new(1, -16, 0, 0)
    lbl.Position = UDim2.new(0, 8, 0, 6)
    lbl.BackgroundTransparency = 1
    lbl.TextWrapped = true
    lbl.AutomaticSize = Enum.AutomaticSize.Y
    lbl.Font = Enum.Font.SourceSans
    lbl.TextSize = 18
    lbl.Text = tostring(text or "")
    lbl.TextXAlignment = Enum.TextXAlignment.Left
    if colorTbl and type(colorTbl) == "table" then
        pcall(function() lbl.TextColor3 = Color3.fromRGB(colorTbl[1], colorTbl[2], colorTbl[3]) end)
    else
        if isUser then lbl.TextColor3 = Color3.fromRGB(86,170,255) else lbl.TextColor3 = Color3.fromRGB(255,255,255) end
    end
    lbl.Parent = fr

    task.defer(function()
        pcall(function()
            gptScroll.CanvasSize = UDim2.new(0,0,0,uiLayout.AbsoluteContentSize.Y)
            gptScroll.CanvasPosition = Vector2.new(0, uiLayout.AbsoluteContentSize.Y)
        end)
    end)
    return fr, lbl
end

local function appendSystem(msg) appendChat(false, "[System] "..tostring(msg), {200,200,255}) end
local function appendError(msg) appendChat(false, "[Error] "..tostring(msg), {255,80,80}) end

-- ===== Client Event (RemoteEvent / BindableEvent fallback) =====
local function ensureClientEvent()
    if ReplicatedStorage then
        local ex = ReplicatedStorage:FindFirstChild("ChatGPTClientEvent")
        if not (ex and ex:IsA("RemoteEvent")) then
            local ok, ev = pcall(function()
                local _ev = Instance.new("RemoteEvent")
                _ev.Name = "ChatGPTClientEvent"
                _ev.Parent = ReplicatedStorage
                return _ev
            end)
            -- if failed to create, ignore (client fallback will use bindable)
        end
    end
    local bindParent = gptGui or (LocalPlayer and LocalPlayer:FindFirstChild("PlayerGui")) or script.Parent
    if not bindParent then return end
    local existing = bindParent:FindFirstChild("ChatGPTClientEvent_Bindable")
    if not existing then
        local b = Instance.new("BindableEvent")
        b.Name = "ChatGPTClientEvent_Bindable"
        b.Parent = bindParent
    end
end
ensureClientEvent()

-- small API to fire/connect
_G.ChatGPTClientEventFire = function(payload)
    pcall(function()
        if ReplicatedStorage and ReplicatedStorage:FindFirstChild("ChatGPTClientEvent") then
            ReplicatedStorage.ChatGPTClientEvent:FireServer(payload)
        end
    end)
    pcall(function()
        local bp = gptGui and gptGui:FindFirstChild("ChatGPTClientEvent_Bindable")
        if bp and bp:IsA("BindableEvent") then bp:Fire(payload) end
    end)
end

-- ===== Execution subsystem (pla:) =====
local virtualInput = pcall(function() return game:GetService("VirtualInputManager") end) and game:GetService("VirtualInputManager") or nil

local function pla_handle_action(action)
    if not action or action == "" then appendError("pla: missing action"); return end
    local ch = LocalPlayer and LocalPlayer.Character
    local hrp = ch and (ch:FindFirstChild("HumanoidRootPart") or ch.PrimaryPart)
    local hum = ch and ch:FindFirstChildOfClass("Humanoid")
    if not hrp or not hum then appendError("No character/HRP/Humanoid available."); return end

    local a = trim(action)
    local low = a:lower()

    -- run.<key>.<mode>  e.g. run.W.Step(3,0.2s) or run.W.Hold or run.W.Stop
    if low:sub(1,4) == "run." then
        local rest = a:sub(5)
        local parts = {}
        for p in rest:gmatch("[^%.]+") do table.insert(parts, trim(p)) end
        local key = (parts[1] or "W"):upper()
        local mode = parts[2] or "Step"
        local function dirVec(k)
            if k == "W" then return hrp.CFrame.LookVector end
            if k == "S" then return -hrp.CFrame.LookVector end
            if k == "A" then return -hrp.CFrame.RightVector end
            if k == "D" then return hrp.CFrame.RightVector end
            return nil
        end
        local dv = dirVec(key)
        if not dv then appendError("Unknown run key: "..tostring(key)); return end
        -- Hold mode
        if mode:lower() == "hold" then
            gptGui._holdTokens = gptGui._holdTokens or {}
            gptGui._holdTokens[key] = true
            appendSystem(("Hold %s started"):format(key))
            task.spawn(function()
                while gptGui._holdTokens and gptGui._holdTokens[key] do
                    pcall(function() hum:MoveTo(hrp.Position + dv * 6) end)
                    task.wait(0.12)
                end
                appendSystem("Hold ended: "..key)
            end)
            return
        elseif mode:lower() == "stop" then
            if gptGui._holdTokens then gptGui._holdTokens[key] = false end
            appendSystem("Hold stop for "..key)
            return
        else
            -- Step mode optionally with params: Step(n,t)
            local n,t = mode:match("Step%((%d+),%s*([%d%.]+)s?%)")
            local repeats = tonumber(n) or 1
            local delay = tonumber(t) or 0.25
            for i=1,repeats do
                pcall(function() hum:MoveTo(hrp.Position + dv * 6) end)
                task.wait(delay)
            end
            appendSystem(("Stepped %s x%d"):format(key, repeats))
            return
        end
    end

    -- chat.<text> or chat.<text>.spam(1s)
    if low:sub(1,5) == "chat." then
        local raw = a:sub(6)
        local spamDel = raw:match("%.spam%(([%d%.]+)s?%)")
        if spamDel then
            local txt = raw:match("^(.-)%.spam") or raw
            local d = tonumber(spamDel) or 1
            if gptGui._chatSpam then gptGui._chatSpam = nil; appendSystem("Chat spam stopped"); return end
            gptGui._chatSpam = true
            appendSystem(("Chat spam started: \"%s\" every %.2fs"):format(txt, d))
            task.spawn(function()
                while gptGui._chatSpam do
                    pcall(function()
                        if ReplicatedStorage and ReplicatedStorage:FindFirstChild("DefaultChatSystemChatEvents") and ReplicatedStorage.DefaultChatSystemChatEvents:FindFirstChild("SayMessageRequest") then
                            ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(txt, "All")
                        else
                            LocalPlayer:Chat(txt)
                        end
                    end)
                    task.wait(d)
                end
            end)
            return
        else
            pcall(function()
                if ReplicatedStorage and ReplicatedStorage:FindFirstChild("DefaultChatSystemChatEvents") and ReplicatedStorage.DefaultChatSystemChatEvents:FindFirstChild("SayMessageRequest") then
                    ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(raw, "All")
                else
                    LocalPlayer:Chat(raw)
                end
            end)
            appendSystem("Sent chat: "..raw)
            return
        end
    end

    -- key.<Key>
    if low:sub(1,4) == "key." then
        local k = a:match("^key%.(.+)")
        if not k then appendError("No key specified"); return end
        if virtualInput then
            pcall(function() virtualInput:SendKeyEvent(true, k, false, game) end)
            task.wait(0.06)
            pcall(function() virtualInput:SendKeyEvent(false, k, false, game) end)
            appendSystem("Virtual key: "..k)
        else
            local map = { W = hrp.CFrame.LookVector, S = -hrp.CFrame.LookVector, A = -hrp.CFrame.RightVector, D = hrp.CFrame.RightVector }
            local dv = map[k:upper()]
            if dv then pcall(function() hum:MoveTo(hrp.Position + dv * 6) end); appendSystem("Fallback moved for "..k) else appendError("No virtual input available for "..k) end
        end
        return
    end

    -- dance.<emote>
    if low:sub(1,6) == "dance." then
        local em = a:match("^dance%.(.+)")
        if em then pcall(function() LocalPlayer:Chat("/e "..em) end); appendSystem("Emote: "..em) end
        return
    end

    appendError("Unknown pla action: "..tostring(action))
end

-- ===== Command parser / handler =====
local function try_parse_parentheses_content(txt)
    local inside = txt:match("%((.*)%)")
    if inside then
        inside = inside:gsub('^%s*"', ""):gsub('"%s*$', ""):gsub("^%s*'",""):gsub("'%s*$","")
        return trim(inside)
    end
    return nil
end

local function split_args_parentheses(s)
    local inside = s:match("%((.*)%)")
    if not inside then return {} end
    local parts = {}
    local cur = ""
    local depth = 0
    for i = 1, #inside do
        local ch = inside:sub(i,i)
        if ch == "," and depth == 0 then
            table.insert(parts, trim(cur)); cur = ""
        else
            cur = cur .. ch
            if ch == "(" then depth = depth + 1 end
            if ch == ")" then depth = math.max(0, depth - 1) end
        end
    end
    if trim(cur) ~= "" then table.insert(parts, trim(cur)) end
    for i,p in ipairs(parts) do
        parts[i] = p:gsub('^%s*"', ""):gsub('"%s*$', ""):gsub("^%s*'",""):gsub("'%s*$","")
    end
    return parts
end

local savedKeeps = {}
local savedViewState = { active = false, conn = nil, prevType = nil, prevSubject = nil }
local chatConnections = { enabled = false, conns = {}, playersConn = nil }
local autoBotEnabled = false
local autoBotTask = nil

local function checkPlayerByName(name)
    if not name or name == "" then return nil end
    name = trim(name)
    local p = Players:FindFirstChild(name)
    if p then return p end
    for _,pl in ipairs(Players:GetPlayers()) do
        if pl.Name and pl.Name:lower():find(name:lower()) then return pl end
    end
    return nil
end

local function handleCommand(rawText)
    if not rawText or rawText == "" then return false end
    local t = tostring(rawText)
    if t:sub(1,1) ~= "/" then return false end
    local cmd, rest = t:match("^/(%S+)%s*(.*)$")
    cmd = (cmd or ""):upper()
    rest = rest or ""

    -- /API
    if cmd == "API" then
        local inside = try_parse_parentheses_content(rest) or trim(rest)
        if inside == "" then appendSystem('Usage: /API ("sk-...")'); return true end
        STATE.OPENAI_KEY = inside; STATE.validated = false
        appendSystem(("API set: %s (not validated)"):format(redactKey(STATE.OPENAI_KEY))); return true
    end

    -- /APIJ
    if cmd == "APIJ" then
        local inside = try_parse_parentheses_content(rest) or trim(rest)
        if inside == "" then appendSystem('Usage: /APIJ ("proj_...")'); return true end
        STATE.PROJECT_ID = inside; appendSystem(("Project ID set: %s"):format(tostring(STATE.PROJECT_ID))); return true
    end

    if cmd == "UNSAVEAPIJ" or cmd == "NOAPIJ" then STATE.PROJECT_ID = nil; appendSystem("Project ID cleared."); return true end

    -- /HttpState
    if cmd == "HTTPSTATE" then
        local urlRaw = try_parse_parentheses_content(rest) or trim(rest)
        if urlRaw == "" then appendSystem('Usage: /HttpState ("example.com" or "https://example.com")'); return true end
        local url = (urlRaw:match("^https?://") and urlRaw) or ("https://" .. urlRaw)
        if not HttpService then appendError("HttpService unavailable."); return true end
        appendSystem("Checking " .. url .. " ...")
        local ok, resp = pcall(function() return HttpService:RequestAsync({ Url = url, Method = "GET", Timeout = 8 }) end)
        if not ok then appendError("Http request failed: "..tostring(resp)); return true end
        if not resp then appendError("HTTP returned no response."); return true end
        appendSystem(("HTTP status: %s | Success: %s"):format(tostring(resp.StatusCode), tostring(resp.Success))); return true
    end

    -- /HttpService
    if cmd == "HTTPSERVICE" then
        if not HttpService then appendError("HttpService unavailable"); return true end
        local ok, enabled = pcall(function() return HttpService.HttpEnabled end)
        appendSystem(("HttpEnabled = %s (pcall ok=%s)"):format(tostring(enabled), tostring(ok))); return true
    end

    if cmd == "HTTPENABLED" then
        local v = trim(rest):lower()
        if v ~= "true" and v ~= "false" then appendSystem('Usage: /HttpEnabled true|false'); return true end
        if not HttpService then appendError("HttpService unavailable"); return true end
        local succ, err = pcall(function() HttpService.HttpEnabled = (v == "true") end)
        local current = nil; pcall(function() current = HttpService.HttpEnabled end)
        if succ then appendSystem(("Set HttpEnabled -> current: %s"):format(tostring(current))) else appendError(("Set HttpEnabled failed: %s | current: %s"):format(tostring(err), tostring(current))) end
        return true
    end

    -- /Keep
    if cmd == "KEEP" then
        local content = try_parse_parentheses_content(rest) or trim(rest)
        if content == "" then appendSystem('Usage: /Keep ("Text")'); return true end
        table.insert(savedKeeps, content)
        appendSystem(("Saved note #%d"):format(#savedKeeps))
        return true
    end
    if cmd == "SHOWKEEP" then
        if #savedKeeps == 0 then appendSystem("No saved notes.") else appendSystem("Saved notes:"); for i,v in ipairs(savedKeeps) do appendChat(false, ("#%d: %s"):format(i, tostring(v)), {200,200,255}) end end; return true
    end
    if cmd == "CLEARALLKEEP" or cmd == "CAK" then savedKeeps = {}; appendSystem("All saved notes cleared."); return true end

    -- /ClearTexts
    if cmd == "CLEARTEXTS" then
        for _,c in ipairs(gptChat:GetChildren()) do
            if c:IsA("Frame") then pcall(function() c:Destroy() end) end
        end
        appendSystem("Cleared chat.")
        return true
    end

    -- /LeaveGame /Exit
    if cmd == "LEAVEGAME" or cmd == "EXIT" then
        appendSystem("Leaving...")
        pcall(function() LocalPlayer:Kick("") end)
        return true
    end

    -- /Reset or /Re
    if cmd == "RESET" or cmd == "RE" then
        local ch = LocalPlayer and LocalPlayer.Character
        if ch then
            local hum = ch:FindFirstChildOfClass("Humanoid")
            if hum then pcall(function() hum.Health = 0 end); appendSystem("Character reset (hum.Health = 0).") else pcall(function() ch:BreakJoints() end); appendSystem("Character broken (BreakJoints).") end
        else
            appendSystem("No character to reset.")
        end
        return true
    end

    -- /View ("pla:Name" or "obj:locate")
    if cmd == "VIEW" or cmd == "SPY" then
        local inside = try_parse_parentheses_content(rest) or trim(rest)
        if inside == "" then appendSystem('Usage: /View ("pla:Name" or "obj:locate")'); return true end
        local targetStr = inside
        local isPla = false
        if targetStr:sub(1,4):lower() == "pla:" then isPla = true; targetStr = targetStr:sub(5) end
        if isPla then
            local pl = checkPlayerByName and checkPlayerByName(targetStr) or Players:FindFirstChild(targetStr)
            if not pl or not pl.Character then appendError("Player not found or no character"); return true end
            local hrp = pl.Character:FindFirstChild("HumanoidRootPart") or pl.Character.PrimaryPart
            if not hrp then appendError("Target has no HRP"); return true end
            if savedViewState.conn then pcall(function() savedViewState.conn:Disconnect() end); savedViewState.conn = nil end
            local cam = workspace.CurrentCamera
            savedViewState.prevType = cam.CameraType
            savedViewState.prevSubject = cam.CameraSubject
            savedViewState.active = true
            savedViewState.conn = RunService.RenderStepped:Connect(function()
                if not hrp or not hrp.Parent then
                    if savedViewState.conn then savedViewState.conn:Disconnect(); savedViewState.conn = nil end
                    savedViewState.active = false
                    return
                end
                pcall(function() cam.CameraType = Enum.CameraType.Scriptable; cam.CFrame = hrp.CFrame * CFrame.new(0,2,6) end)
            end)
            appendSystem(("Now viewing %s (use /UnView)."):format(pl.Name))
            return true
        else
            local obj, err = resolveLocate(targetStr)
            if not obj then appendError("Object not found: "..tostring(err)); return true end
            local rootPart = (obj:IsA("Model") and (obj:FindFirstChild("HumanoidRootPart") or obj.PrimaryPart)) or (obj:IsA("BasePart") and obj)
            if not rootPart then appendError("Object not viewable"); return true end
            if savedViewState.conn then pcall(function() savedViewState.conn:Disconnect() end); savedViewState.conn = nil end
            local cam = workspace.CurrentCamera
            savedViewState.prevType = cam.CameraType
            savedViewState.prevSubject = cam.CameraSubject
            savedViewState.active = true
            savedViewState.conn = RunService.RenderStepped:Connect(function()
                if not rootPart or not rootPart.Parent then
                    if savedViewState.conn then savedViewState.conn:Disconnect(); savedViewState.conn = nil end
                    savedViewState.active = false
                    return
                end
                pcall(function() cam.CameraType = Enum.CameraType.Scriptable; cam.CFrame = rootPart.CFrame * CFrame.new(0,2,6) end)
            end)
            appendSystem(("Now viewing object %s (use /UnView)."):format(targetStr))
            return true
        end
    end

    if cmd == "UNVIEW" or cmd == "UNSPY" then
        if savedViewState.active then
            if savedViewState.conn then pcall(function() savedViewState.conn:Disconnect() end); savedViewState.conn = nil end
            local cam = workspace.CurrentCamera
            pcall(function()
                cam.CameraType = savedViewState.prevType or Enum.CameraType.Custom
                cam.CameraSubject = savedViewState.prevSubject or (LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid"))
            end)
            savedViewState.active = false
            appendSystem("Camera restored.")
        else
            appendSystem("Not currently viewing anyone.")
        end
        return true
    end

    -- /SeeChat /UnSeeChat
    if cmd == "SEECHAT" or cmd == "ENACHAT" then
        if chatConnections.enabled then appendSystem("SeeChat already enabled"); return true end
        chatConnections.enabled = true; chatConnections.conns = {}
        local function bindPlayer(p)
            if not p then return end
            pcall(function()
                local c = p.Chatted:Connect(function(msg) appendChat(false, ("[%s]: %s"):format(p.Name, tostring(msg)), {200,200,200}) end)
                chatConnections.conns[p] = c
            end)
        end
        for _,p in ipairs(Players:GetPlayers()) do bindPlayer(p) end
        chatConnections.playersConn = Players.PlayerAdded:Connect(bindPlayer)
        appendSystem("SeeChat enabled"); return true
    end
    if cmd == "UNSEECHAT" or cmd == "UNENACHAT" then
        if not chatConnections.enabled then appendSystem("SeeChat not enabled"); return true end
        chatConnections.enabled = false
        if chatConnections.playersConn then pcall(function() chatConnections.playersConn:Disconnect() end); chatConnections.playersConn = nil end
        for p,c in pairs(chatConnections.conns or {}) do pcall(function() c:Disconnect() end) end
        chatConnections.conns = {}
        appendSystem("SeeChat disabled"); return true
    end

    -- /Ins (basic)
    if cmd == "INS" then
        local args = split_args_parentheses(rest)
        if #args < 3 then appendSystem('Usage: /Ins ("Class","Name","ParentLocate")'); return true end
        local className, setName, parentLocate = args[1], args[2], args[3]
        local parent, err = resolveLocate(parentLocate)
        if not parent then appendError("Parent not found: "..tostring(err)); return true end
        local ok, inst = pcall(function() return Instance.new(className) end)
        if not ok or not inst then appendError("Create failed for "..tostring(className)); return true end
        pcall(function() inst.Name = setName; inst.Parent = parent end)
        appendSystem(("Created %s named %s under %s"):format(className, tostring(setName), tostring(parent)))
        return true
    end

    -- /Destroy
    if cmd == "DESTROY" then
        local args = split_args_parentheses(rest)
        if #args < 1 then appendSystem('Usage: /Destroy ("locate" or "Name")'); return true end
        local targetStr = args[1]
        local obj, err = resolveLocate(targetStr)
        if not obj then obj = findObjectByName(targetStr) end
        if not obj then appendError("Object not found: "..tostring(err)); return true end
        pcall(function() obj:Destroy() end)
        appendSystem(("Destroyed %s"):format(targetStr))
        return true
    end

    -- /Resize
    if cmd == "RESIZE" then
        local args = split_args_parentheses(rest)
        if #args < 2 then appendSystem('Usage: /Resize ("locate","Vector3(x,y,z)")'); return true end
        local locate, vecStr = args[1], args[2]
        local obj, err = resolveLocate(locate)
        if not obj then appendError("Locate failed: "..tostring(err)); return true end
        local vec, verr = safeEval(vecStr)
        if not vec or typeof(vec) ~= "Vector3" then appendError("Vector parse failed"); return true end
        pcall(function() if obj:IsA("BasePart") then obj.Size = vec end end)
        appendSystem(("Resized %s"):format(locate)); return true
    end

    -- /Position
    if cmd == "POSITION" or cmd == "REPOS" or cmd == "REPOSITION" then
        local args = split_args_parentheses(rest)
        if #args < 2 then appendSystem('Usage: /Position ("locate","Vector3(x,y,z)")'); return true end
        local locate, vecStr = args[1], args[2]
        local obj, err = resolveLocate(locate)
        if not obj then appendError("Locate failed: "..tostring(err)); return true end
        local vec, verr = safeEval(vecStr)
        if not vec or typeof(vec) ~= "Vector3" then appendError("Vector parse failed"); return true end
        pcall(function() if obj:IsA("BasePart") then obj.CFrame = CFrame.new(vec) end end)
        appendSystem(("Moved %s"):format(locate)); return true
    end

    -- /Color /ReColor
    if cmd == "COLOR" or cmd == "RECOLOR" then
        local args = split_args_parentheses(rest)
        if #args < 2 then appendSystem('Usage: /Color ("locate","Color3.fromRGB(255,0,0)")'); return true end
        local locate, colorStr = args[1], args[2]
        local obj, err = resolveLocate(locate)
        if not obj then appendError("Locate failed: "..tostring(err)); return true end
        local col, cerr = safeEval(colorStr)
        if not col then
            local nums = {}
            for n in colorStr:gmatch("%d+") do table.insert(nums, tonumber(n)) end
            if #nums == 3 then col = Color3.fromRGB(nums[1], nums[2], nums[3]) end
        end
        if not col or typeof(col) ~= "Color3" then appendError("Color parse failed"); return true end
        applyColorToObject(obj, col)
        appendSystem(("Color set for %s"):format(locate)); return true
    end

    -- /TP (Teleport target to Vector3)
    if cmd == "TP" or cmd == "TELEPORT" then
        local inside = try_parse_parentheses_content(rest) or trim(rest)
        if inside == "" then appendSystem('Usage: /TP ("Vector3(x,y,z)")'); return true end
        local vec, err = safeEval(inside)
        if not vec or typeof(vec) ~= "Vector3" then appendError("Vector parse failed: "..tostring(err)); return true end
        local ch = LocalPlayer and LocalPlayer.Character
        if not ch then appendError("No character"); return true end
        local root = ch:FindFirstChild("HumanoidRootPart") or ch.PrimaryPart
        if not root then appendError("No root part"); return true end
        local ok, e = pcall(function() root.CFrame = CFrame.new(vec) end)
        if ok then appendSystem("Teleport attempted (CFrame)"); return true end
        local hum = ch:FindFirstChildOfClass("Humanoid")
        if hum then pcall(function() hum:MoveTo(vec) end); appendSystem("Teleport fallback MoveTo called") else appendError("Teleport failed: "..tostring(e)) end
        return true
    end

    -- /Rejoin /RJ
    if cmd == "REJOIN" or cmd == "RJ" then
        appendSystem("Rejoining...")
        if TeleportService then pcall(function() TeleportService:Teleport(game.PlaceId, LocalPlayer) end) else appendError("TeleportService unavailable") end
        return true
    end

    -- /ServerHop (basic)
    if cmd == "SERVERHOP" or cmd == "SH" then
        local inside = try_parse_parentheses_content(rest) or trim(rest)
        local limit = tonumber(inside:match("plalimit:(%d+)")) or tonumber(inside) or 8
        appendSystem(("ServerHop: searching servers with players < %d ..."):format(limit))
        if not HttpService then appendError("HttpService unavailable"); return true end
        local url = ("https://games.roblox.com/v1/games/%s/servers/Public?sortOrder=Asc&limit=100"):format(tostring(game.PlaceId))
        local ok, body = pcall(function() return HttpService:GetAsync(url, true) end)
        if not ok or not body then appendError("Server list fetch failed"); return true end
        local ok2, data = pcall(function() return HttpService:JSONDecode(body) end)
        if not ok2 or not data or not data.data then appendError("Invalid server data"); return true end
        for _,server in ipairs(data.data) do
            if server.id and (server.playing or 0) < limit and server.id ~= game.JobId then
                appendSystem(("Hopping to server %s"):format(server.id))
                pcall(function() TeleportService:TeleportToPlaceInstance(game.PlaceId, server.id, LocalPlayer) end)
                return true
            end
        end
        appendError("No server matching criteria")
        return true
    end

    -- /HttpAsync
    if cmd == "HTTPASYNC" then
        local v = trim(rest):lower()
        if v ~= "true" and v ~= "false" then appendSystem('Usage: /HttpAsync true|false'); return true end
        STATE.HttpAsync = (v == "true")
        appendSystem(("HttpAsync -> %s"):format(tostring(STATE.HttpAsync)))
        return true
    end

    -- /AutoBot true/false
    if cmd == "AUTOBOT" then
        local inside = try_parse_parentheses_content(rest) or trim(rest)
        local enable = inside:lower():find("true") ~= nil
        if enable and not autoBotEnabled then
            autoBotEnabled = true
            appendSystem("AutoBot started")
            autoBotTask = task.spawn(function()
                local emotes = {"dance","wave","cheer","laugh","point"}
                while autoBotEnabled do
                    local ch = LocalPlayer and LocalPlayer.Character
                    local hrp = ch and (ch:FindFirstChild("HumanoidRootPart") or ch.PrimaryPart)
                    local hum = ch and ch:FindFirstChildOfClass("Humanoid")
                    if hrp and hum then
                        local r = math.random()
                        if r < 0.45 then pcall(function() hum:MoveTo(hrp.Position + Vector3.new(math.random(-5,5),0,math.random(-5,5))) end)
                        elseif r < 0.75 then pcall(function() hum.Jump = true end)
                        else pcall(function() LocalPlayer:Chat("/e "..emotes[math.random(1,#emotes)]) end)
                        end
                        if math.random() < 0.25 then pcall(function() hrp.CFrame = hrp.CFrame * CFrame.Angles(0, math.rad(180 + math.random(-90,90)), 0) end) end
                    end
                    task.wait(math.random(1,3))
                end
                appendSystem("AutoBot stopped")
            end)
            return true
        elseif not enable and autoBotEnabled then
            autoBotEnabled = false
            autoBotTask = nil
            appendSystem("AutoBot stopping")
            return true
        else
            appendSystem("AutoBot state unchanged")
            return true
        end
    end

    -- /TextsCounts
    if cmd == "TEXTSCOUNTS" or cmd == "TEXTSCOUNT" then
        local count = 0
        for _,c in ipairs(gptChat:GetChildren()) do if c:IsA("Frame") and c.Name:match("^ChatItem_%d+") then count = count + 1 end end
        appendSystem(("Total messages: %d"):format(count))
        return true
    end

    -- /Translate
    if cmd == "TRANSLATE" then
        local inside = try_parse_parentheses_content(rest) or trim(rest)
        if inside:lower():match("^global:") then STATE.translateGlobal = inside:sub(8):upper(); appendSystem(("Global translate -> %s"):format(STATE.translateGlobal)); return true end
        appendSystem('Usage: /Translate ("global:EN" or "global:TH")'); return true
    end

    -- /Execute "pla:..." or "cmd:..."
    if cmd == "EXECUTE" then
        local inside = try_parse_parentheses_content(rest) or trim(rest)
        if inside == "" then appendSystem('Usage: /execute ("pla:..." or "cmd:...")'); return true end
        if inside:sub(1,4):lower() == "pla:" then pla_handle_action(inside:sub(5)); return true end
        if inside:sub(1,4):lower() == "cmd:" then local c = inside:sub(5); if c:sub(1,1)~="/" then c = "/"..c end; pcall(function() handleCommand(c) end); return true end
        appendError("execute target must be pla: or cmd:")
        return true
    end

    -- /Help
    if cmd == "HELP" then
        appendSystem("Commands (summary):")
        appendChat(false, '/API ("sk-...") /APIJ ("proj_...") /UnSaveAPIJ', {200,200,255})
        appendChat(false, '/HttpService /HttpState ("url") /HttpEnabled true|false /HttpAsync true|false', {200,200,255})
        appendChat(false, '/Keep /ShowKeep /ClearAllKeep /ClearTexts /TextsCounts', {200,200,255})
        appendChat(false, '/LeaveGame /Reset /View /UnView /SeeChat /UnSeeChat', {200,200,255})
        appendChat(false, '/Ins /InsMesh /InsDecal /InsSurfaceGui /InsEffect', {200,200,255})
        appendChat(false, '/Destroy /Resize /Position /Color /TP /Rejoin /ServerHop', {200,200,255})
        appendChat(false, '/AutoBot true|false /Translate /Execute "pla:..." or "cmd:..." /Console', {200,200,255})
        appendChat(false, '/Instance shorten to /Ins; see usage for parameters', {200,200,255})
        return true
    end

    appendError("Unknown command: /" .. tostring(cmd) .. " (use /Help)")
    return true
end

-- ===== Bind top buttons (proj/api) =====
pcall(function()
    if projBtn and projBtn:IsA("TextButton") then
        projBtn.MouseButton1Click:Connect(function()
            local v = trim(projBox.Text or "")
            if v == "" then appendError("Project ID empty"); return end
            if not v:match("^proj_") then appendError("Project ID should start with 'proj_'"); return end
            STATE.PROJECT_ID = v; appendSystem("Project ID saved")
        end)
        projBox.FocusLost:Connect(function(enter) if enter then projBtn:Activate() end end)
    end
    if apiBtn and apiBtn:IsA("TextButton") then
        apiBtn.MouseButton1Click:Connect(function()
            local raw = trim(apiBox.Text or "")
            if raw == "" then appendError("API key empty"); return end
            STATE.OPENAI_KEY = raw; appendSystem(("API key set: %s (not validated)"):format(redactKey(raw)))
            -- optional quick validate (best-effort)
            if HttpService then
                task.spawn(function()
                    local ok, resp = pcall(function()
                        local payload = { model = STATE.MODEL, messages = {{ role="user", content="Hello" }}, max_tokens = 1 }
                        return HttpService:RequestAsync({ Url = "https://api.openai.com/v1/chat/completions", Method = "POST", Headers = { ["Content-Type"]="application/json", ["Authorization"]="Bearer "..raw, ["OpenAI-Project"]=(STATE.PROJECT_ID or "") }, Body = HttpService:JSONEncode(payload), Timeout = 12 })
                    end)
                    if ok and resp and resp.Success then STATE.validated = true; appendSystem("API validated (quick check)") else appendError("API validation failed (quick check)") end
                end)
            end
        end)
        apiBox.FocusLost:Connect(function(enter) if enter then apiBtn:Activate() end end)
    end
end)

-- ===== OpenAI call =====
local function callOpenAI_simple(prompt)
    if not HttpService then return nil, "HttpService unavailable" end
    if not STATE.OPENAI_KEY or STATE.OPENAI_KEY == "" then return nil, "no_key" end
    local payload = { model = STATE.MODEL, messages = {{ role = "user", content = prompt }}, max_tokens = 400, temperature = 0.6 }
    local headers = { ["Content-Type"] = "application/json", ["Authorization"] = "Bearer " .. tostring(STATE.OPENAI_KEY) }
    if STATE.PROJECT_ID and STATE.PROJECT_ID ~= "" then headers["OpenAI-Project"] = tostring(STATE.PROJECT_ID) end
    local ok, resp = pcall(function()
        return HttpService:RequestAsync({
            Url = "https://api.openai.com/v1/chat/completions",
            Method = "POST",
            Headers = headers,
            Body = HttpService:JSONEncode(payload),
            Timeout = 25,
        })
    end)
    if not ok then return nil, tostring(resp) end
    if not resp or not resp.Success then
        local parsed = nil; pcall(function() parsed = HttpService:JSONDecode(resp.Body or "{}") end)
        local errMsg = parsed and parsed.error and parsed.error.message or ("status:"..tostring(resp.StatusCode))
        return nil, errMsg
    end
    local decoded = nil; pcall(function() decoded = HttpService:JSONDecode(resp.Body or "{}") end)
    if decoded and decoded.choices and decoded.choices[1] and decoded.choices[1].message and decoded.choices[1].message.content then
        return tostring(decoded.choices[1].message.content), nil
    end
    return nil, "no_content"
end

-- ===== Send chat =====
local function sendChat()
    local text = tostring(chatBox.Text or "")
    if text:match("^%s*$") then return end

    if text:sub(1,1) == "/" then
        local handled = false
        local ok, res = pcall(function() return handleCommand(text) end)
        if ok and res then handled = true end
        if handled then chatBox.Text = "" ; return end
        -- if not handled, fallback to console (handled below)
    end

    appendChat(true, text)
    chatBox.Text = ""

    if not STATE.OPENAI_KEY or STATE.OPENAI_KEY == "" then
        appendError("Error HttpService: Did you forget to put API?")
        return
    end

    appendChat(false, "[System] ‚åõ Thinking...")
    task.spawn(function()
        local reply, err = callOpenAI_simple(text)
        if reply then
            if STATE.translateGlobal then
                -- best-effort translate using OpenAI
                local ok, tr = pcall(function()
                    local prompt = ("Translate the following text to %s (only translation):\n\n%s"):format(STATE.translateGlobal, reply)
                    local r, e = callOpenAI_simple(prompt)
                    return r, e
                end)
                if ok and tr then appendChat(false, tr) else appendChat(false, reply) end
            else
                appendChat(false, reply)
            end
        else
            appendError("OpenAI Error: "..tostring(err))
        end
    end)
end

pcall(function() if sendBtn and sendBtn:IsA("TextButton") then sendBtn.MouseButton1Click:Connect(sendChat) end end)
pcall(function() if chatBox and chatBox:IsA("TextBox") then chatBox.FocusLost:Connect(function(enter) if enter then sendChat() end end) end end)

-- ===== Toggle binding (use existing gpt button if any) =====
local function findToggleBtn()
    local btn = nil
    pcall(function()
        if type(menuGui) == "Instance" and menuGui.Parent then btn = menuGui:FindFirstChild("a3_ChatGPT", true) or menuGui:FindFirstChild("gpt", true) end
        if not btn then btn = parentRoot:FindFirstChild("a3_ChatGPT", true) or parentRoot:FindFirstChild("gpt", true) end
    end)
    return btn
end
local toggleBtn = findToggleBtn()
local panelOpen = false
local function togglePanel()
    panelOpen = not panelOpen
    if panelOpen then safeTween(gptGui, { Position = UDim2.new(0,0,0.1,0) }, 0.28) else safeTween(gptGui, { Position = UDim2.new(-1,0,0.1,0) }, 0.28) end
end
if toggleBtn and (toggleBtn:IsA("ImageButton") or toggleBtn:IsA("TextButton")) then pcall(function() toggleBtn.MouseButton1Click:Connect(togglePanel) end) end

-- ===== Console UI (optional) =====
-- (omitted for brevity here; the previous code included a console; you can re-add if needed)

appendSystem("ChatGPT UI ready. Use /Help for commands.")

-- Expose small API
_G.ExperienceSettings_GPT_STATE = STATE
_G.ExperienceSettings_ToggleGPT = togglePanel

-- End of LocalScript
